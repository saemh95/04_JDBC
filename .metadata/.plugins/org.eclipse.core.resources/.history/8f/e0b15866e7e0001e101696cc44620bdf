package edu.kh.jdbc1;


import java.util.*;
import java.sql.*;
public class JDBCExample3 {

	public static void main(String[] args) {
		
		Connection conn = null;
		Statement stmt = null;
		ResultSet rs = null;
		
		try {
			
			
		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			
			try {
				
				if (conn != null) conn.close();
				if (stmt != null) stmt.close();
				if (rs != null) rs.close();
				
			} catch (Exception e) {
				e.printStackTrace();
			}
			
		}
		
	}
	
}
